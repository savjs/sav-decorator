{
  "name": "sav-decorator",
  "version": "0.0.1",
  "description": "Interface generator with ES7 decorator",
  "main": "dist/sav-decorator.cjs.js",
  "module": "dist/sav-decorator.es.js",
  "jsnext:main": "dist/sav-decorator.es.js",
  "scripts": {
    "build": "rollup -c",
    "nyc": "nyc ava ./test",
    "lint": "standard rollup.config.js test/** src/**",
    "start": "watch -p \"+(test|src)/*.js\" -c \"npm run lint -- --fix && npm run build && nyc ava ./test\"",
    "test": "npm run lint && npm run build && nyc ava ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savjs/sav-decorator.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savjs/sav-decorator/issues"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "expect.js": "^0.3.1",
    "nyc": "^10.0.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.7.1",
    "sav-util": "0.0.9",
    "standard": "^8.5.0",
    "watch-cli": "^0.2.1"
  },
  "dependencies": {
    "path-to-regexp": "^1.7.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ]
    }
  },
  "homepage": "https://github.com/savjs/sav-decorator#readme"
}
